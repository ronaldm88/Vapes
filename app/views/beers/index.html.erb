<%= link_to "Add Beer", new_beer_path, class:"btn btn-success"  %>
<br><br>
<!--
  Turn Into Table, maybe last 10?
-->
<% if @beers.nil? %>
  <p>
    No beers in the database yet.
  </p>
<% else %>
  <% @beers.each do |beer| %>
    <%= link_to beer.name, beer_path(beer) %> - <%= link_to beer.brewery, beers_path %><br>
  <% end %>
<% end %>
