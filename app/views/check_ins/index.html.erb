<div class="row">
  <div class="col-md-6">
    <button type="button" name="button" class="btn btn-success" id="add-check-in">Add Check In</button>

    <h2>Last Ten Check-Ins</h2>

    <div class="check-ins-list">
      <ul>
        <% @check_ins.each do |check_in| %>
          <li><%= display_check_in(check_in) %></li>
        <% end %>
      </ul>
    </div>
  </div>

  <div class="col-md-6" id="new-check-in-form" style="display:none;">
    <%= render 'index_form', check_in: @check_in %>
  </div>
</div>
